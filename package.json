{
  "name": "react-h-tag",
  "version": "1.0.1",
  "license": "MIT",
  "author": "Benjamin Johnson",
  "main": "dist/index.js",
  "module": "dist/react-h-tag.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint src",
    "size": "yarn build && size-limit"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "300 B"
    },
    {
      "path": "dist/react-h-tag.esm.js",
      "limit": "300 B"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.6",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "husky": "^3.0.9",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  }
}
